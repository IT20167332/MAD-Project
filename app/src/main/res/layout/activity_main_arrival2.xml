<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background33"
    tools:context=".Activity.Main_arrival">

    <TextView
        android:id="@+id/textView"
        android:layout_width="170dp"
        android:layout_height="67dp"
        android:text="@string/Arrivaltxt"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/sdsd"
        android:layout_width="360dp"
        android:layout_height="182dp"
        android:layout_marginTop="16dp"
        android:background="#E4D291"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="123dp"
            android:layout_height="29dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="Vehicle Number:"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="24dp"
            android:text="Arrival Time:"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/textView31"
            app:layout_constraintTop_toBottomOf="@+id/textView31" />

        <TextView
            android:id="@+id/idid2"
            android:layout_width="185dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:text="WP-2070"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView31"
            app:layout_constraintStart_toEndOf="@+id/textView31" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="182dp"
            android:layout_height="32dp"
            android:layout_marginStart="20dp"
            android:text="2:30 PM"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView32"
            app:layout_constraintStart_toEndOf="@+id/textView32" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Accept"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="360dp"
        android:layout_height="182dp"
        android:layout_marginTop="24dp"
        android:background="#E4D291"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdsd">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="123dp"
            android:layout_height="29dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="Vehicle Number:"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="24dp"
            android:text="Arrival Time:"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/textView31"
            app:layout_constraintTop_toBottomOf="@+id/textView31" />

        <TextView
            android:id="@+id/idid2"
            android:layout_width="185dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:text="WP-2070"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView31"
            app:layout_constraintStart_toEndOf="@+id/textView31" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="182dp"
            android:layout_height="32dp"
            android:layout_marginStart="20dp"
            android:text="2:30 PM"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView32"
            app:layout_constraintStart_toEndOf="@+id/textView32" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Accept"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="360dp"
        android:layout_height="182dp"
        android:layout_marginTop="32dp"
        android:background="#E4D291"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="123dp"
            android:layout_height="29dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="Vehicle Number:"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="24dp"
            android:text="Arrival Time:"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/textView31"
            app:layout_constraintTop_toBottomOf="@+id/textView31" />

        <TextView
            android:id="@+id/idid2"
            android:layout_width="185dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:text="WP-2070"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView31"
            app:layout_constraintStart_toEndOf="@+id/textView31" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="182dp"
            android:layout_height="32dp"
            android:layout_marginStart="20dp"
            android:text="2:30 PM"
            android:textSize="24sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView32"
            app:layout_constraintStart_toEndOf="@+id/textView32" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Accept"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.865"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView36" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>